   <types1:Item>
      <types1:EntryId>bdtqSLlBWlD2pHA-gt-5492876-2020-02-21T10:38:04.002Z::pd.xml</types1:EntryId>
      <types1:Name>SCM_Import_from_Git</types1:Name>
